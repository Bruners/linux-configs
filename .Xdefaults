Xft.lcdfilter: lcddefault
Xft.dpi: 96
Xft.rgba: rgba
Xft.hinting: true
Xft.hintstyle: hintlight
Xft.antialias: true

#include "/home/lasseb/.xcolors/solarized.xcolors"

URxvt*font: xft:Inconsolata:regular:autohint=true:pixelsize=13
URxvt*boldFont: xft:Inconsolata:bold:autohint=true:pixelsize=13
URxvt*italicFont: xft:Inconsolata:italic:autohint=true:pixelsize=13
URxvt*boldItalicFont: xft:Inconsolata:bold:italic:autohint=true:pixelsize=13

!URxvt*font: xft:Anonymous Pro:antialias=true:hinting=false:pixelsize=13
!URxvt*boldFont: xft:Anonymous Pro B:antialias=true:hinting=false:pixelsize=13
!URxvt*italicFont: xft:Anonymous Pro I:antialias=true:hinting=false:pixelsize=13
!URxvt*bolditalicFont: xft:Anonymous Pro BI:antialias=true:hinting=false:pixelsize=13

URxvt*depth: 		32
URxvt*background: 	rgba:0000/0000/0000/aaaa
URxvt*shading:		25
URxvt*transparent:	false
URxvt*saveLines:	32767
URxvt*visualBell:	true
URxvt*scrollBar:	False
URxvt*cursorColor:	#ffcc00
URxvt*loginShell:	true
URxvt*termName:		rxvt-unicode
URxvt*print-pipe:	cat > $(echo urxvt.dump.$(date +'%Y%M%d%H%m%S'))
URxvt*secondaryScroll:	true
!URxvt*mapAlert:		true
!URxvt*utmpInhibit:	true
URxvt*scrollTtyOutput: 	false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
!URxvt*borderLess:	true
!URxvt*skipBuiltinGlyphs: true
!URxvt*mouseWheelScrollPage: true
URxvt*pointerBlank: true
URxvt*pointerBlankDelay: 5
URxvt*urgentOnBell:	true
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~
URxvt*perl-lib:           /usr/lib/urxvt/perl/
URxvt*perl-ext-common:    matcher
URxvt*urlLauncher:        /usr/bin/firefox
URxvt*matcher.button:     1
URxvt*cutchars: `"()'*<>[]{|}

IRC*title:              IRC

Xcursor.theme: Obsidian
Xcursor*size: 24
