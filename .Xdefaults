! rxvt-unicode ---------------------------------------------------------------

!urxvt*font: -xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso8859-1
!urxvt*boldFont: -xos4-terminus-bold-r-normal--12-120-72-72-c-60-iso8859-1
urxvt*boldFont: -windows-dina-bold-r-normal--13-80-96-96-c-70-microsoft-cp1252
urxvt*font: -windows-dina-medium-r-normal--13-80-96-96-c-70-microsoft-cp1252
urxvt*internalBorder:     5
urxvt*fading:             20
!darken=(0 to 100) lighten=(-1 to -100)
urxvt*shading:            30
!urxvt*tintColor:          black
!urxvt*depth:             32
urxvt*saveLines:          32767
urxvt*visualBell:         true
urxvt*scrollTtyKeypress:  true
urxvt*scrollWithBuffer:   false
urxvt*scrollTtyOutput:    false
urxvt*scrollBar:          false
urxvt*scrollstyle:        xterm
urxvt*scrollBar_right:    true
urxvt*scrollColor:        #777777
urxvt*cursorColor:        #ffcc00
urxvt*loginShell:         true
urxvt*termName:           rxvt-unicode
urxvt*transparent:        true
urxvt*shadomg:            10
urxvt*cutchars:           "()*,<>[]{}|'"
urxvt*print-pipe:         cat > $(echo urxvt.dump.$(date +'%Y%M%d%H%m%S'))
urxvt*secondaryScroll:    true
urxvt*mapAlert:           true
urxvt*utmpInhibit:        true

urxvt*perl-lib:           /usr/lib/urxvt/perl/
urxvt*perl-ext-common:    default,matcher
urxvt*urlLauncher:        /usr/bin/firefox
urxvt*matcher.button:     1

xcalc*font: -windows-dina-bold-r-normal--13-80-96-96-c-70-microsoft-cp1252

!transparent=0000 opaque=ffff
urxvt*background: rgba:0000/0000/0000/4444
urxvt*intensityStyles: True

Xcursor*theme: whiteglass
Xcursor*size: 16

xterm*eightBitInput: False
xterm*font: -*-dina-medium-r-*-*-*-*-*-*-*-*-*-*
xterm*dynamicColors: True
xterm*utf8: 2
xterm*saveLines: 32767
xterm*scrollBar: False
xterm*loginShell: True
xterm*scrollTtyKeypress: True
xterm*scrollTtyOutput: True
xterm*jumpScroll: True
xterm*mutiScroll: True
xterm*toolBar: false

xscreensaver.Dialog.headingFont:        -*-dina-bold-r-*-*-13-*-*-*-*-*-*-*
xscreensaver.Dialog.bodyFont:           -*-dina-medium-r-*-*-13-*-*-*-*-*-*-*
xscreensaver.Dialog.labelFont:          -*-dina-medium-r-*-*-13-*-*-*-*-*-*-*
xscreensaver.Dialog.unameFont:          -*-dina-medium-r-*-*-13-*-*-*-*-*-*-*
xscreensaver.Dialog.buttonFont:         -*-dina-bold-r-*-*-13-*-*-*-*-*-*-*
xscreensaver.Dialog.dateFont:           -*-dina-medium-r-*-*-13-*-*-*-*-*-*-*

xscreensaver.passwd.passwdFont:         -*-dina-bold-r-*-*-13-*-*-*-*-*-*-*
!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         #ffffff
xscreensaver.Dialog.background:         #111111
xscreensaver.Dialog.topShadowColor:     #111111
xscreensaver.Dialog.bottomShadowColor:  #111111
xscreensaver.Dialog.Button.foreground:  #666666
xscreensaver.Dialog.Button.background:  #ffffff
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #666666
xscreensaver.Dialog.text.background:    #ffffff
xscreensaver.Dialog.internalBorderWidth:24
xscreensaver.Dialog.borderWidth:        20
xscreensaver.Dialog.shadowThickness:    2
!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #ff0000
xscreensaver.passwd.thermometer.background:  #000000
xscreensaver.passwd.thermometer.width:       8
!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %a %b %d, %Y

