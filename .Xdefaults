URxvt*font:		xft:Monospace:pixelsize=11:antialias=true:hinting=true
URxvt*boldFont:		xft:Monospace:pixelsize=11:bold:antialias=true:hinting=true
URxvt.depth: 24
URxvt*shading:		30
URxvt*transparent:	true
URxvt*saveLines:	4096
URxvt*visualBell:	true
URxvt*scrollBar:	False
URxvt*cursorColor:	#ffcc00
URxvt*loginShell:	true
URxvt*termName:		rxvt-unicode
URxvt*print-pipe:	cat > $(echo urxvt.dump.$(date +'%Y%M%d%H%m%S'))
URxvt*secondaryScroll:	true
!URxvt*mapAlert:		true
!URxvt*utmpInhibit:	true
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
!URxvt*borderLess:	true
!URxvt*skipBuiltinGlyphs: true
URxvt*mouseWheelScrollPage: false
URxvt*pointerBlank: true
URxvt*pointerBlankDelay: 5
URxvt*urgentOnBell:	true
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~
URxvt*perl-lib:           /usr/lib/urxvt/perl/
URxvt*perl-ext-common:    matcher
URxvt*urlLauncher:        /usr/bin/firefox
URxvt*matcher.button:     1

