Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.hintstyle: hintfull

#include "/home/lasseb/.xcolors/solarized.xcolors"

!URxvt*font:		xft:Monospace:pixelsize=11:antialias=true:hinting=true
!URxvt*boldFont:		xft:Monospace:pixelsize=11:bold:antialias=true:hinting=true

URxvt*font: xft:Anonymous Pro:antialias=true:hinting=true:pixelsize=11
URxvt*boldFont: xft:Anonymous Pro B:antialias=true:hinting=true:pixelsize=11
URxvt*italicFont: xft:Anonymous Pro I:antialias=true:hinting=true:pixelsize=11
URxvt*bolditalicFont: xft:Anonymous Pro BI:antialias=true:hinting=true:pixelsize=11

URxvt*depth: 		32
URxvt*background: 	rgba:0000/0000/0000/aaaa
URxvt*shading:		25
URxvt*transparent:	false
URxvt*saveLines:	32767
URxvt*visualBell:	true
URxvt*scrollBar:	False
URxvt*cursorColor:	#ffcc00
URxvt*loginShell:	true
URxvt*termName:		rxvt-unicode
URxvt*print-pipe:	cat > $(echo urxvt.dump.$(date +'%Y%M%d%H%m%S'))
URxvt*secondaryScroll:	true
!URxvt*mapAlert:		true
!URxvt*utmpInhibit:	true
URxvt*scrollTtyOutput: 	false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
!URxvt*borderLess:	true
!URxvt*skipBuiltinGlyphs: true
!URxvt*mouseWheelScrollPage: true
URxvt*pointerBlank: true
URxvt*pointerBlankDelay: 5
URxvt*urgentOnBell:	true
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~
URxvt*perl-lib:           /usr/lib/urxvt/perl/
URxvt*perl-ext-common:    matcher
URxvt*urlLauncher:        /usr/bin/firefox
URxvt*matcher.button:     1
URxvt*cutchars: `"()'*<>[]{|}

IRC*title:              IRC

Xcursor*theme: bluecurve
Xcursor*size: 16
