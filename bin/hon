#!/bin/sh
HON='hon-x86'

if ps ax | grep -v grep | grep $HON > /dev/null
then
   pid=`ps aux | grep $HON | grep -v grep | awk '{print $2}'`
   echo "HoN is already running on $pid, killing processÂ $pid"
   kill -9 $pid
   /games/hon/hon.sh &
else
  echo "Starting HoN and mumble"
  mumble mumble://o.haaland.net:64738/?version=1.2.3 &
  /games/hon/hon.sh &
fi

